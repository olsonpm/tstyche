{
  "cache": {
    "cacheLocation": "./.cache/cspell",
    "useCache": true
  },
  "files": [
    "**/*.js",
    "**/*.json",
    "**/*.md",
    "examples/**/*",
    "models/**/*",
    "source/**/*",
    "tests/**/*",
    "typetests/**/*"
  ],
  "ignorePaths": [
    "**/__snapshots__/**/*"
  ],
  "language": "en-US",
  "useGitignore": true,
  "words": [
    "codacy",
    "dprint",
    "lcovonly",
    "packagephobia",
    "tsserverlibrary",
    "tstyche",
    "typecheck",
    "typetests"
  ]
}
